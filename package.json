{
  "name": "numerology-server",
  "version": "0.1.1",
  "description": "NodeJS project for Simple Numerology App",
  "main": "app.js",
  "scripts": {
    "test": "mocha --require \"db/test/setup.js\"",
    "dev": "nodemon db/src/server.js",
    "migrate": "postgrator --config postgrator-config.js",
    "migrate-production": "postgrator --config postgrator-production-config.js",
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node db/src/server.js",
    "client": "node start-client.js",
    "predeploy": "npm audit && npm run migrate-production",
    "deploy": "git push heroku master",
    "postdeploy": "now alias -A client/build/now.json"
  },
  "keywords": [],
  "engines": {
    "node": "10.15.1"
  },
  "author": "matrayu",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "helmet": "^3.18.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.18.4",
    "morgan": "^1.9.1",
    "pg": "^7.11.0",
    "treeize": "^2.1.2",
    "winston": "^3.2.1",
    "xss": "^1.0.6"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "concurrently": "^4.1.1",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.1",
    "postgrator-cli": "^3.1.0",
    "supertest": "^4.0.2"
  }
}
